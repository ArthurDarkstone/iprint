{
  "name": "@iprint/shared",
  "type": "module",
  "version": "0.0.0-beta.0",
  "packageManager": "pnpm@10.13.1",
  "description": "browser printing library",
  "author": "Arthur Darkstone<arthurlli@qq.com>",
  "license": "MIT",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "dev": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./utils": {
      "dev": "./src/utils.ts",
      "default": "./dist/utils.js"
    },
    "./*": "./*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "*.d.mts",
    "*.js",
    "*.mjs"
  ],
  "engines": {
    "node": ">=20.18.0"
  },
  "buildOptions": {
    "name": "PrintShared",
    "formats": [
      "esm-bundler",
      "esm-browser",
      "cjs",
      "global"
    ]
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.js",
      "./utils": "./dist/utils.js",
      "./*": "./*"
    }
  }
}
