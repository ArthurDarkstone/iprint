{
  "name": "@iprint/core",
  "type": "module",
  "version": "0.0.0-beta.0",
  "packageManager": "pnpm@10.13.1",
  "description": "browser printing library",
  "author": "Arthur Darkstone<arthurlli@qq.com>",
  "license": "MIT",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "dev": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./iprint": {
      "dev": "./src/iprint.ts",
      "default": "./dist/iprint.js"
    },
    "./*": "./*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "*.d.mts",
    "*.js",
    "*.mjs"
  ],
  "engines": {
    "node": ">=20.18.0"
  },
  "scripts": {},
  "dependencies": {
    "@iprint/shared": "workspace:*"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.js",
      "./iprint": "./dist/iprint.js",
      "./*": "./*"
    }
  }
}
