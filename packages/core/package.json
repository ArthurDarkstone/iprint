{
  "name": "@iprint/core",
  "type": "module",
  "version": "0.0.0-beta.0",
  "packageManager": "pnpm@10.13.1",
  "description": "browser printing library",
  "author": "Arthur Darkstone<arthurlli@qq.com>",
  "license": "MIT",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "dev": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./container": {
      "dev": "./src/container.ts",
      "default": "./dist/container.js"
    },
    "./element": {
      "dev": "./src/element.ts",
      "default": "./dist/element.js"
    },
    "./movable": {
      "dev": "./src/movable.ts",
      "default": "./dist/movable.js"
    },
    "./print": {
      "dev": "./src/print.ts",
      "default": "./dist/print.js"
    },
    "./renderer": {
      "dev": "./src/renderer.ts",
      "default": "./dist/renderer.js"
    },
    "./*": "./*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "*.d.mts",
    "*.js",
    "*.mjs"
  ],
  "engines": {
    "node": ">=20.18.0"
  },
  "scripts": {},
  "dependencies": {
    "@iprint/shared": "workspace:*"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.js",
      "./container": "./dist/container.js",
      "./element": "./dist/element.js",
      "./movable": "./dist/movable.js",
      "./print": "./dist/print.js",
      "./renderer": "./dist/renderer.js",
      "./*": "./*"
    }
  }
}
